<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Xyngr @ViewBag.Title </title>
    <link rel="shortcut icon" href="/images/favicon.png" />
    <link href="https://maxcdn.icons8.com/fonts/line-awesome/1.1/css/line-awesome.min.css" rel="stylesheet">
    @Styles.Render("~/Content/AdminCss")
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/Admin/Xyngr.js"></script>
    @Scripts.Render("~/bundles/AdminJS")
</head>
<body class="vertical-layout vertical-menu-modern 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu-modern" data-col="2-columns">
    @Html.Partial("~/Views/Admin/_Header.cshtml")
    @Html.Partial("~/Views/Admin/_Menu.cshtml")
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                @RenderBody()
            </div>
        </div>
    </div>
    @Html.Partial("~/Views/Admin/_Footer.cshtml")
    @RenderSection("scripts", required: false)


</body>
</html>
