@model xyngr.Models.ContactUsViewModel
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!--====== LOCATON ==========-->
<section>
    <div class="rows contact-map map-container">
        <iframe src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDClyAC1U-y_X06_OjJQqRoxjt2_zuqxSw&q=@Model.GeneralSettings.Address" allowfullscreen></iframe>
    </div>
</section>
<!--====== QUICK ENQUIRY FORM ==========-->
<section class="tourb2-ab-p-2 com-colo-abou">
    <div class="container">
        <!-- TITLE & DESCRIPTION -->
        <div class="spe-title col-md-12">
            <h2><span>Contact us</span></h2>
            <div class="title-line">
                <div class="tl-1"></div>
                <div class="tl-2"></div>
                <div class="tl-3"></div>
            </div>
            @*<p>World's leading tour and travels Booking website,Over 30,000 packages worldwide. Book travel packages and enjoy your holidays with distinctive experience</p>*@
        </div>
        <div class="row tourb2-ab-p1">
            <div class="col-md-12 col-sm-12">
                <div class="tourb2-ab-p1-left">
                    <p>
                        @Html.Raw(Model.StaticPages.PageContent)
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>
<section>
    <div class="form form-spac rows con-page">
        <div class="container">
            <!-- TITLE & DESCRIPTION -->
            

            <div class="pg-contact">
                <div class="col-md-3 col-sm-6 col-xs-12 new-con new-con1">
                    @*<h2>The <span>Travel</span></h2>
                    <p>We Provide Outsourced Software Development Services To Over 50 Clients From 21 Countries.</p>*@
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 new-con new-con1">
                    <img src="img/contact/1.png" alt="">
                    <h4>Address</h4>
                    <p>
                        @Model.GeneralSettings.Address
                    </p>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 new-con new-con3">
                    <img src="img/contact/2.png" alt="">
                    <h4>CONTACT INFO:</h4>
                    <p>
                        <a href="tel://@Model.GeneralSettings.SupportPhone" class="contact-icon">Phone: @Model.GeneralSettings.SupportPhone</a>
                        <br> <a href="mailto:@Model.GeneralSettings.SupportEmail" class="contact-icon">Email: @Model.GeneralSettings.SupportEmail</a>
                    </p>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 new-con new-con4">
                    @*<img src="img/contact/3.png" alt="">
                    <h4>Website</h4>
                    <p>
                        <a href="#">Website: www.mycompany.com</a>
                        <br> <a href="#">Facebook: www.facebook/my</a>
                        <br> <a href="#">Blog: www.blog.mycompany.com</a>
                    </p>*@
                </div>
            </div>
        </div>
    </div>
</section>