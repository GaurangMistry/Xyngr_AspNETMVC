@model xyngr.Models.StaticPagesViewModel
@{
    ViewBag.Title = Model.PageName;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section>
    <div class="rows inner_banner inner_banner_2">
        <div class="container">
            <h2><span>@Model.PageName</span></h2>
            <ul>
                <li>
                    <a href="/">Home</a>
                </li>
                <li><i class="fa fa-angle-right" aria-hidden="true"></i> </li>
                <li>
                    <a href="#inner-page-title" class="bread-acti">@Model.PageName</a>
                </li>
            </ul>
            @*<p>Book travel packages and enjoy your holidays with distinctive experience</p>*@
        </div>
    </div>
</section>
<section class="tourb2-ab-p-2 com-colo-abou">
    <div class="container">
        <!-- TITLE & DESCRIPTION -->
        <div class="spe-title">
            <h2>Privacy Policy</h2>
            <div class="title-line">
                <div class="tl-1"></div>
                <div class="tl-2"></div>
                <div class="tl-3"></div>
            </div>
            @*<p>World's leading tour and travels Booking website,Over 30,000 packages worldwide. Book travel packages and enjoy your holidays with distinctive experience</p>*@
        </div>
        <div class="row tourb2-ab-p1">
            <div class="col-md-12 col-sm-12">
                <div class="tourb2-ab-p1-left">
                    <p>
                        @Html.Raw(Model.PageContent)
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>
