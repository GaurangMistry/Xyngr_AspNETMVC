@model IEnumerable<xyngr.Models.CategoryViewModel>

<div class="top-logo" data-spy="affix" data-offset-top="250" id="divMenu">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="wed-logo">
                    <a href="/"></a>
                </div>
                <div class="main-menu">
                    <ul id="ulMenu">
                        <li>
                            <a href="/Home">Home</a>
                        </li>
                        @foreach (var item in Model)
                        {
                            <li>
                                <a href="/Venue/@item.CategoryName.Replace(" ","").Replace("-","").Replace("&","and")-@item.CategoryID">@item.CategoryName.Replace(" and "," & ") </a>
                            </li>
                        }
                        @*<li>
                            <a href="/AboutUs">About Us</a>
                        </li>
                        <li>
                            <a href="/ContactUs">Contact us</a>
                        </li>*@
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
