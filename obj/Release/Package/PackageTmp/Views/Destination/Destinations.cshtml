@model xyngr.Models.BranchListingForFrontViewModel
@{
    ViewBag.Title = Model.Business.Category;
    ViewBag.ParentTitle = Model.Business.ParentCategory;

    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section header {
    @if (!Model.Business.MetaAuthorContent.Equals(string.Empty))
    {
        <meta name="Author" content="@Model.Business.MetaAuthorContent" />
    }
    @if (!Model.Business.MetaDescContent.Equals(string.Empty))
    {
        <meta name="Description" content="@Model.Business.MetaDescContent" />
    }
    @if (!Model.Business.MetaKeyWordContent.Equals(string.Empty))
    {
        <meta name="Keywords" content="@Model.Business.MetaKeyWordContent" />
    }

    @*// Open Graph*@
    <meta content="@Model.Business.ImageURL" property="og:image" />
    <meta content="Website" property="og:type" />
    <meta content="www.xyngr.com" property="og:site_name" />
    <meta content="@Model.Business.Category" property="og:title" />
    <meta content="@Model.Business.Category" property="og:description" />
    <meta content="@Request.Url" property="og:url" />

    @*// Twitter*@
    <meta name='twitter:card' content="Destination" />
    <meta name='twitter:site' content="www.xyngr.com" />
    <meta name='twitter:title' content="@Model.Business.Category" />
    <meta name='twitter:description' content="@Model.Business.Category" />
    <meta name='twitter:image' content="@Model.Business.ImageURL" />

    @*// Canonical*@
    <link href="@Request.Url" rel="canonical" />
}

@Html.Partial("~/Views/Destination/_DestinationPartial.cshtml", Model)